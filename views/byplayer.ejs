                              <tr>
                                  <td class="left"><p><strong>Profit</strong></p></td>
                                  <td><h4><%= carlicTotals.avgProfit.toFixed(2) %></h4></td>
                                  <td><h4><%= carlicTotals.total %></h4></td>
                              </tr>
                              <tr>
                                    <td></td>
                                      <td class="sessions-tag"><p>Sessions:</p></td>
                                      <td class="sessions-tag"><p><%= carlicTotals.sessions %></p></td>
                                  </tr>
                            </tbody>
                        </table>
                    </div>
                    
            
    </body>
</html>